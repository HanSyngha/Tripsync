{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/privacy",
        "destination": "/privacy-policy.html"
      },
      {
        "source": "/privacy-en",
        "destination": "/privacy-policy-en.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "tripsync-e7591",
          "appId": "1:75226997618:android:c3214d9a681053f232b3eb",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "tripsync-e7591",
          "configurations": {
            "android": "1:75226997618:android:c3214d9a681053f232b3eb",
            "ios": "1:75226997618:ios:ed077a6c9a1d2dac32b3eb"
          }
        }
      }
    }
  }
}
